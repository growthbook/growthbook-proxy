{
  "name": "growthbook-proxy",
  "engines": {
    "node": ">=16"
  },
  "description": "GrowthBook proxy server for caching, realtime updates, telemetry, etc",
  "private": true,
  "version": "1.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/growthbook/growthbook-proxy"
  },
  "author": "Bryce Fitzsimons",
  "scripts": {
    "lint": "eslint './**/*.{ts,js}' --fix --max-warnings 0 --cache --cache-strategy content",
    "build:clean": "rimraf -rf dist",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'dist/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'dist/cjs' --source-maps",
    "build:bundles": "rollup -c",
    "build": "tsc",
    "type-check": "tsc --pretty --noEmit",
    "start": "node dist/src/index.js",
    "dev": "concurrently 'tsc --watch' 'nodemon -q dist/src/index.js | yarn pino-pretty'"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "got": "11.8.3",
    "http-proxy-middleware": "^2.0.6",
    "mongodb": "^4.13.0",
    "pino-http": "^8.3.1",
    "redis": "^4.5.1",
    "spdy": "^4.0.2",
    "sse-pubsub": "^1.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/pino": "^7.0.5",
    "@types/redis": "^4.0.11",
    "@types/spdy": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "4.7.4"
  }
}
