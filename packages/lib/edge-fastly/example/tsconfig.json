{
  "compilerOptions": {
    "strict": true,
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": [ "es2022" ],
    "outDir": "./build",
    "skipLibCheck": true,
    "paths": {
      "@growthbook/edge-fastly": ["../src/index.ts"]
    }
  },
  "include": [
    "./src/**/*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}
